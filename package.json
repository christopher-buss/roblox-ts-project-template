{
	"name": "roblox-ts-project-template",
	"type": "module",
	"version": "1.0.0",
	"packageManager": "pnpm@9.3.0",
	"description": "A Roblox game made with React and Roblox-TS",
	"author": "",
	"license": "ISC",
	"keywords": [],
	"main": "index.js",
	"scripts": {
		"build-prod": "npm run clean && cross-env NODE_ENV=production npx rbxtsc --verbose && npm run darklua",
		"sync-prod": "rojo serve ./build.project.json",
		"build-dev": "cross-env NODE_ENV=development npx rbxtsc",
		"watch-dev": "cross-env NODE_ENV=development npx rbxtsc -w",
		"sync-dev": "rojo serve",
		"clean": "npx rimraf out/ && npx rimraf dist",
		"darklua": "darklua process out dist --verbose",
		"eslint-config": "npx @eslint/config-inspector",
		"lint": "eslint --no-config-lookup --config eslint.config.js --max-warnings 0 ."
	},
	"dependencies": {
		"@flamework/components": "^1.2.3",
		"@flamework/core": "^1.2.3",
		"@flamework/networking": "^1.2.3",
		"@isentinel/eslint-config": "^0.5.7",
		"@rbxts/inspect": "^1.0.1",
		"@rbxts/janitor": "1.15.7-ts.0",
		"@rbxts/lapis": "^0.3.0",
		"@rbxts/log": "^0.6.3",
		"@rbxts/make": "^1.0.6",
		"@rbxts/message-templates": "^0.3.2",
		"@rbxts/object-utils": "^1.0.4",
		"@rbxts/pretty-react-hooks": "^0.5.0",
		"@rbxts/promise-child": "^1.2.1",
		"@rbxts/rbx-debug": "^1.0.1",
		"@rbxts/react": "^0.4.0",
		"@rbxts/react-reflex": "^0.3.4",
		"@rbxts/react-roblox": "^0.4.0",
		"@rbxts/reflex": "^4.3.1",
		"@rbxts/remap": "^0.2.1",
		"@rbxts/ripple": "^0.8.1",
		"@rbxts/services": "^1.5.4",
		"@rbxts/sift": "^0.0.9",
		"@rbxts/signal": "^1.1.1",
		"@rbxts/t": "^3.1.1",
		"@rbxts/ui-labs": "^2.1.1",
		"@rbxts/validate-tree": "^2.0.2",
		"rbxts-transform-debug": "^2.2.0",
		"rbxts-transform-env": "^2.2.0"
	},
	"devDependencies": {
		"@eslint-react/eslint-plugin": "1.5.16",
		"@eslint/config-inspector": "0.4.11",
		"@rbxts/compiler-types": "2.3.0-types.1",
		"@rbxts/types": "1.0.781",
		"bundle-require": "5.0.0",
		"cross-env": "7.0.3",
		"eslint": "9.5.0",
		"eslint-config-flat-gitignore": "0.1.5",
		"eslint-plugin-format": "0.1.2",
		"eslint-plugin-react-hooks": "4.6.2",
		"eslint-plugin-simple-import-sort": "12.1.0",
		"lint-staged": "15.2.7",
		"prettier-plugin-jsdoc": "1.3.0",
		"rbxts-transformer-flamework": "1.2.3",
		"rimraf": "5.0.7",
		"roblox-ts": "2.3.0-dev-e6e7658",
		"simple-git-hooks": "2.11.1",
		"typescript": "5.4.5"
	},
	"pnpm": {
		"patchedDependencies": {
			"roblox-ts@2.3.0-dev-e6e7658": "patches/roblox-ts@2.3.0-dev-e6e7658.patch",
			"rbxts-transform-env@2.2.0": "patches/rbxts-transform-env@2.2.0.patch"
		}
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
	},
	"lint-staged": {
		"*": "eslint --fix"
	}
}
